  z-index: 99;
}
.vedioPlayBox .boxCenter .active::after{
  content: '';
  height: 60rpx;
  width: 8rpx;
  background:  #FF633A;
  top: 30rpx;
  left: 0;
  position: absolute;
}
.vedioPlayBox .boxCenter .active .userName{
    color: #FF633A;
}
.vedioPlayBox .boxCenter .active .videoName{
    color: #FF633A;
}
.isLoding{
