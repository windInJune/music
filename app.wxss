}
.loginPage .agreebutton{
  width: 430rpx;
  height: 138rpx;
  position: absolute;
  bottom: 100rpx;
  left: 50%;
  margin-left: -215rpx;
}
.loginPage .agreebutton image{
  width: 100%;
  height: 100%;
}
.loginPage .agreebutton button{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  opacity: 0;
}




.footer_player{
  width: 100%;
  height: 130rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
  background-color: rgba(255, 255, 255, 1);
  box-shadow: 0rpx 0rpx 5rpx #eee;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 999;
}
@keyframes singer{ 
  0%{
		transform: rotate(0deg);
	}
	100%{
		transform: rotate(360deg);
	}
}
.footer_player .singer_cover{
  width: 130rpx;
  height: 130rpx;
  border-radius: 50%;
  border:10rpx solid #fff;
  overflow: hidden;
  box-shadow: 5rpx 7rpx 10rpx #eee;
  background: #fff;
  position: absolute;
  bottom: 10rpx;
  left: 10rpx;
}
.footer_player .singer_cover image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
  animation:singer 10s linear infinite;
}
.footer_player .controls{
  width: 590rpx;
  height: 100%;
  padding: 20rpx 30rpx 20rpx 20rpx;
  box-sizing: border-box;
  position: absolute;
  right: 0;
}
.footer_player .singer_info{
  width: 350rpx;
  font-size: 34rpx;
  padding: 10rpx 0;
  float: left;
  /* background-color: #f99; */
}
.footer_player .sing_name{
  font-size: 32rpx;
  color: #FF633A;
  line-height: 50rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.footer_player .singer_name{
  font-size: 24rpx;
  color: #333;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.footer_player .btn{
  width: 180rpx;
  height: 100rpx;
  position: absolute;
  right: 0;
  bottom: 0;
}
.footer_player .btn .playBtn{
  width: 56rpx;
  height: 56rpx;
  position: absolute;
  left: 0;
  top: 50%;
  margin-top: -28rpx;
  
}
.footer_player .btn .showListIcon{
  width: 56rpx;
  height: 56rpx;
   position: absolute;
  right: 30rpx;
  top: 50%;
  margin-top: -28rpx;
}


.vedioPlayBox{
  height: 560rpx;
  position: fixed;
  width: 100%;
  background: #fff;
  bottom: 0;
  right: 0;
  z-index: 999;
}

.vedioPlayBox .boxTop,.vedioPlayBox .boxBtm{
  height: 100rpx;
  line-height: 100rpx;
  position: absolute;
  left: 0;
}
 .vedioPlayBox .boxTop{
  top: 0;
  width: 100%;
  padding-left: 30rpx;
  box-sizing: border-box;
  font-size: 32rpx;
} 
.vedioPlayBox .boxBtm{
  bottom: 0;
  width: 100%;
  text-align: center;
  font-size: 32rpx;
  border-top: 1px solid #f2f2f2;
}
.vedioPlayBox .boxCenter{
    height:360rpx;
    position: absolute;
    left: 0;
    top: 100rpx;
}
.vedioPlayBox .boxCenter .ctListBox{
   height: 120rpx;
   border-top: 1px solid #f2f2f2;
   position: relative;
   box-sizing: border-box;
   padding:10rpx 30rpx;
}

.vedioPlayBox .boxCenter .ctListBox .videoName{
  height: 60rpx;
  line-height: 60rpx;
  font-size: 32rpx;
  padding-right: 200rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.vedioPlayBox .boxCenter .ctListBox  .userName{
   height: 40rpx;
  line-height: 40rpx;
  font-size: 24rpx;
  color: #A1A1A1;
}
 .vedioPlayBox  .boxTop image{
   width: 30rpx;
   height: 30rpx;
    margin-right: 18rpx;
   vertical-align: middle;
 }
 .vedioPlayBox .boxBtm{
  bottom: 0;
} 
.overView{
  height: 120rpx;
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 9;
}
