page{
  width: 100%;
  background-color: #F1F1F1;
  font-family: "Microsoft YaHei","黑体","宋体",sans-serif;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
} 

.overhide{
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.goHome{
  position: fixed;
  width:100rpx;
  height: 100rpx;
  border-radius: 50%;
  text-align: center;
  line-height: 100rpx;
  box-shadow: 1px 1px 1px  #949494; 
  background: #FF633A;
  color:#fff;
  font-size: 32rpx;
  z-index: 9999999;
}


.authorizedBox{
  width: 472rpx;
  height: 282rpx;
  position: fixed;
  z-index: 99999;
  margin-top: -200rpx;
  margin-left: -236rpx;
  top: 50%;
  left: 50%;
  background: #fff;
  border-radius: 3px;
}
.authorizedBox .authorizedMsg{
  font-size:32rpx;
  text-align: center;
  height: 80rpx;
  padding: 60rpx 30rpx 0;
}
.authorizedBox .sureBox{
  font-size:34rpx;
  text-align: center;
  height: 100rpx;
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
}

.authorizedBox .sureBox .agree{
   height: 100rpx;
  line-height: 100rpx;
  font-size: 32rpx;
  border-top: 1px solid #f2f2f2;  
  color: #FF633A;
  text-align: center;  border-radius: 0;
  background: #fff;

}

.pubCover{
  position: fixed;
  height: 100%;
  width: 100%;
  background: rgba(0, 0, 0, 0.2);
  bottom: 0;
  right: 0;
  z-index: 9999;
}
.loginPage{
  height: 100%;
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999999999999;
}
.loginPage .bgimage{
  height: 100%;
  width:100%;
}
.logoBox{
    width: 100%;
  height: 500rpx;
  position: absolute;
  top: 0rpx;
  left: 0rpx;
  text-align: center;
}

.logo{
  margin: 100rpx auto 0;
  height: 213rpx;
  width: 213rpx;
}
.logo image{
  width: 100%;
