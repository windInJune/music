page{
  height: 100%;
  overflow: hidden;
}
.bgImg{
  height: 100%;
  width: 100%;
   position: absolute;
  left: 0;
  top: 0;
  z-index: 1;
}
.container{
  height: 80%;
  width: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 2;
  
}
.container view{
  height: 80rpx;
  line-height: 80rpx;
  width: 100%;
  text-align: center;
  font-size: 32rpx;
  color: #fff;
  opacity: .6;
}
.container .active{
  opacity: 1;
  font-size: 42rpx;
}
.footer{
  width: 100%;
  height: 180rpx;
  box-sizing: border-box;
  /* background: red; */
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 9;
}
.footer .btn{
  display: flex;
  width: 100%;
  height: 60rpx;
  margin-top: 40rpx;
  justify-content:space-around;
}
.footer .btn image:active{
  opacity: 0.5;
  filter: blur(4px);
}
.lickIcon{
  width: 60rpx;
  height: 60rpx;
}
.prevIcon{
  width: 60rpx;
  height: 60rpx;
}
.playIcon{
  width: 80rpx;
  height: 80rpx;
  transform: translateY(-10rpx);
  -webkit-transform: trfanslateY(-10rpx);
}
.nextIcon{
  width: 60rpx;
  height: 60rpx;
}
.listIcon{
  width: 56rpx;
  height: 56rpx;
}
.top{
  height: 30rpx;
  width: 100%;
  display: flex;
  justify-content: space-around;
  align-items: center;
  color: #fff;
  font-size: 28rpx;
}
.top view{
  margin-top: -6rpx;
}
.top progress{
  width: 70%;
}
.pubIcon{
  position: fixed;
  right: 30rpx;
  width: 70rpx;
  height: 70rpx;
  bottom: 240rpx;
  z-index: 9;
}
.pubIcon image{
  width: 100%;
  height: 100%;
} 
.loveIcon{
  bottom: 360rpx;  
}
.addIcon{
  bottom: 480rpx;
}
.sharePlayer{
  height: 100%;
  width: 100%;
  position: absolute;
  left: 0;
  top: 0;
  opacity: 0;
}


.addSongListBox{
    height: 500rpx;
  position: fixed;
  width: 100%;
  background: #fff;
  bottom: 0;
  right: 0;
  z-index: 999;
}

.addSongListBox .boxTop{
  top: 0;
  position: absolute;     
  width: 100%;
  text-align: center;
  font-size: 32rpx;
  height: 100rpx;
  line-height: 100rpx;
}
.addSongListBox .boxCenter{
   height:300rpx;
   position: absolute;
   left: 0;
    top: 100rpx;
}
.ctListTop image{
  width: 45rpx;
  height: 45rpx;
  vertical-align: middle;
  margin-right: 30rpx;
}
.addSongListBox .ctListTop{
     height: 100rpx;
     line-height: 100rpx;
   border-top: 1px solid #f2f2f2;
   position: relative;
   box-sizing: border-box;
   padding-left:30rpx;
   
}
.addSongListBox .ctListBox{
     height: 100rpx;
     line-height: 100rpx;
   border-top: 1px solid #f2f2f2;
   position: relative;
   box-sizing: border-box;
   padding-left:92rpx;
   
}
.addSongListBox .boxBtm{
  position: absolute;
  bottom: 0;
  width: 100%;
    height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 32rpx;
  border-top: 1px solid #f2f2f2;
}

.addSongList{
  width: 472rpx;
  height: 282rpx;
  position: fixed;
  z-index: 999;
  margin-top: -200rpx;
  margin-left: -236rpx;
  top: 50%;
  left: 50%;
  background: #fff;
  border-radius: 3px;
}
.addSongList .addSongListName{
  font-size:34rpx;
  text-align: center;
  height: 80rpx;
  line-height: 80rpx;
}
.addSongList  input{
  height: 84rpx;
  width: 80%;
  margin: 0 auto;
  border-radius: 3px;
  background: #f2f2f2;
  padding: 0 15rpx;
}
.addSongList .sureBox{
  font-size:34rpx;
  text-align: center;
  height: 100rpx;
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  overflow: hidden;
  clear: both;
}
.addSongList .sureBox .left{
  float: left;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 32rpx;
  width: 50%;
  border-top: 1px solid #f2f2f2;
  border-right: 1px solid #f2f2f2;
  text-align: center;
  box-sizing: border-box;
  color: #999;
}
.addSongList .sureBox .right{
  float: right;
   height: 100rpx;
  line-height: 100rpx;
  font-size: 32rpx;
  border-top: 1px solid #f2f2f2;  
  width: 50%;  
  color: #FF633A;
  text-align: center;  
}
.musicplay{
  width: 560rpx;
  height: 560rpx;
  position: absolute;
  top: 230rpx;
  left: 50%;
  margin-left: -280rpx;
  z-index: 7;

}
.time{
  width: 15%;
  text-align: center;
}
.musicplay image{
  height: 100%;
  width: 100%;
    animation:singer 10s linear infinite;

}