page{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  background-color:#f2f2f2;
}
.tab{
  width:100%;
  height: 36px;
  background-color:#FF633A;
  overflow: hidden;
  font-size: 34rpx;
  color: #fff;
  position: fixed;
  top: 0rpx;
  left: 0;
  z-index: 999;
}
.tab .left{
  width: 50%;
  height: 36px;
  line-height: 36px;
  text-align: center;
  opacity: 0.8;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 2;
}
.tab .right{
  width: 50%;
  height: 36px;
  line-height: 36px;
  text-align: center;
  opacity: 0.8;
  position: absolute;
  top: 0;
  left: 50%;
  z-index: 2;
}
.tab .checked{
  opacity: 1;
}
 @keyframes toL{ 
  0%{left: 50%;}
	100%{left: 0;}
}
@keyframes toR{ 
  0%{left: 0;}
	100%{left: 50%;}
}
.tab .block{
  display: none;
  width: 20%;
  margin: 0 15%;
  height: 33px;
  border-bottom: 3px solid #fff;
  position: absolute;
  top: 0;
  z-index: 1;
}
.tab .toL{
  display: block;
  animation:toL 0.3s linear;
}
.tab .toR{
  display: block;
  animation:toR 0.3s linear;
} 
.swiper{
  flex: 1;
  width: 100%;
}
