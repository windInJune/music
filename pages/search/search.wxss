page{
  width: 100%;
  height: 100%;
  background-color: #f2f2f2;
}
.searchBox{
  width: 100%;
  padding: 10px 30rpx;
  box-sizing: border-box;
  background: #FF633A;
  overflow: auto;
  position: fixed;
  top: 0;
  left: 0;
}
.searchBox .input{
  width: 590rpx;
  height: 42px;
  /* padding: 22rpx 30rpx; */
  box-sizing: border-box;
  background: #FFF0EC;
  border-radius: 10rpx;
  float: left;
  position: relative;
}
.searchBox .input .searchImg{
  width: 40rpx;
  height: 40rpx;
  float: left;
  position: relative;
  top: 50%;
  margin-top: -20rpx;
  left: 30rpx;
}
.searchBox .input .clearValue{
  width: 40rpx;
  height: 40rpx;
  float: right;
  position: absolute;
  top: 50%;
  margin-top: -20rpx;
  right: 30rpx;
}
.searchBox .input input{
  width: 400rpx;
  height: 42px;
  color: #FF633A;
  font-size: 30rpx;
  padding-left: 10rpx;
  box-sizing: border-box;
  position: relative;
  top: 0rpx;
  left: 30rpx;
}
.searchBox .input .placeHolder{
  color: #FF633A;
}
.searchBox .btn{
  /* width: 120rpx;  */
  height: 42px;
  line-height: 84rpx;
  text-align: right;
  font-size: 34rpx;
  color: #fff;
  float: right;
}
.history{
  width: 100%;
  padding:20rpx 30rpx 0;
  box-sizing: border-box;
  background: #fff;
}
.history .title{
  font-size: 30rpx;
  color: #a1a1a1;
}
.history .title .del{
  width: 160rpx;
  float: right;
  text-align: right;
  font-size: 30rpx;
  color: #FF633A;
}
.history .sList{
  width: 100%;
  height: 100rpx;
  padding: 30rpx 0;
  box-sizing: border-box;
  font-size: 34rpx;
  color: #333;
  border-bottom: 1rpx solid #f2f2f2;
}
.history .sList .clock{
  width: 38rpx;
  height: 38rpx;
  float: left;
}
.history .sList .text{
  width: 500rpx;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 36rpx;
  color: #333;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  float: left;
  margin-left: 18rpx;
}
.history .sList .del{
  width: 40rpx;
  height: 40rpx;
  color: #999;
  float: right;
}
.list{
  width: 100%;
  background: #fff;
  padding:20rpx 30rpx 0;
  box-sizing: border-box;
}
.list .title{
  font-size: 30rpx;
  color: #a1a1a1;
}
.theOne{
  width: 690rpx;
  padding: 26rpx 0;
  overflow: hidden;
  background: #fff; 
  padding-left: 30rpx;
  margin-left: -30rpx;
  border-bottom: 1rpx solid #f2f2f2;
  position: relative;
}
.theOne .name{
  font-size: 36rpx;
  color: #333;
}
.theOne .singer{
  font-size: 26rpx;
  color: #a1a1a1;
  margin-top: 16rpx;
}
.loadMore{
  max-width: 260rpx;
  height:1rpx;
  margin: 40rpx auto;
  text-align: center;
  font-size: 28rpx;
  color: #999;
  line-height: 1rpx;
  border-left: 200rpx solid #ddd;
  border-right: 200rpx solid #ddd;
}

.theOne.active::after{
  content: '';
  height: 70rpx;
  width: 8rpx;
  background:  #FF633A;
  top: 50%;
  margin-top: -35rpx;
  left: 0;
  position: absolute;
}
.theOne.active .singer,.theOne.active .name{
  color: #FF633A;
}